{
  "name": "@aroleaf/djs-bot",
  "version": "5.2.7",
  "decription": "A bot wrapper for discord.js",
  "main": "dist/index.js",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs"
  },
  "commands": "test.js",
  "scripts": {
    "build": "rm -rf dist && npx swc src -d dist && node buildExports.js",
    "types": "rm -rf types && npx tsc --declaration --emitDeclarationOnly --declarationDir types",
    "docs": "npx typedoc",
    "all": "npm run build && npm run types && npm run docs"
  },
  "license": "MIT",
  "devDependencies": {
    "@aroleaf/tsconfig": "^1.0.0",
    "@types/node": "^20.2.5",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.62",
    "typedoc": "^0.23.28",
    "typedoc-plugin-mdn-links": "^2.0.2",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@aroleaf/arguments": "^0.2.5",
    "@aroleaf/parser": "^2.5.0",
    "@swc/helpers": "^0.5.1",
    "discord.js": "^14.11.0",
    "file-ez": "^1.0.2",
    "xregexp": "^5.1.1"
  },
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "types"
  ]
}
